        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".View.Activity.SelectCarActivity"></activity>
        <activity android:name=".View.Activity.CarRegistrationActivity" />
        <activity android:name=".View.Activity.NotificationActivity" />
        <activity android:name=".View.Activity.MenuActivity" />
        <activity android:name=".View.Activity.SignupActivity" />
        <activity android:name=".View.Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.Activity.HealthScoreActivity" />
        <activity android:name=".View.Activity.HelpActivity" />
        <activity android:name=".View.Activity.ShowHistoryActivity" />
        <activity android:name=".View.Activity.ExternalGuideActivity" />
        <activity android:name=".View.Activity.AddHistoryActivity" />
        <activity android:name=".View.Activity.InternalGuideActivity" />
        <activity android:name=".View.Activity.ProblemActivity" />
        <activity android:name=".View.Activity.JournalActivity" />
        <activity android:name=".View.Activity.GuideActivity" />
        <activity android:name=".View.Activity.TroubleshootActivity" />
        <activity android:name=".View.Activity.BaseActivity" />
        <activity android:name=".View.Activity.HomeActivity" />
    </application>

</manifest>